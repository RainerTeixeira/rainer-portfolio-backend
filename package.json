{
  "name": "rainer-portfolio-backend",
  "version": "0.0.1",
  "description": "API para fornecer comunicação com o DynamoDB",
  "author": "Rainer Teixeira",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "if exist dist rmdir /s /q dist || echo Diretório dist não encontrado, continuando...",
    "build": "nest build",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "ts-node-dev --require tsconfig-paths/register --respawn --transpile-only src/main.ts",
    "start:prod": "node dist/main.js",
    "lint": "eslint 'src/**/*.ts' --fix"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.741.0",
    "@aws-sdk/lib-dynamodb": "^3.741.0",
    "@aws-sdk/util-dynamodb": "^3.738.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.1",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/platform-fastify": "^11.0.7",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "aws-jwt-verify": "^5.0.0",
    "aws-lambda": "^1.0.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.7",
    "jsonwebtoken": "^9.0.2",
    "reflect-metadata": "^0.2.2",
    "serverless-http": "^3.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@types/aws-lambda": "^8.10.147",
    "@types/node": "^22.10.7",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "serverless-esbuild": "^1.54.6",
    "serverless-offline": "^14.4.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RainerTeixeira/rainer-portfolio-backend.git"
  },
  "keywords": [],
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/RainerTeixeira/rainer-portfolio-backend/issues"
  },
  "homepage": "https://github.com/RainerTeixeira/rainer-portfolio-backend#readme"
}