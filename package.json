{
  "name": "@rainer/portfolio-backend",
  "version": "4.1.1",
  "description": "Blog Backend com NestJS + Fastify + DynamoDB + MongoDB + AWS Lambda",
  "main": "dist/main.js",
  "type": "commonjs",
  "author": {
    "name": "Rainer Teixeira",
    "email": "suporte@rainersoft.com.br",
    "url": "https://rainersoft.com.br"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "nest start --watch",
    "build": "nest build",
    "start": "node dist/main.js",
    "start:prod": "node dist/main.js",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prisma:generate": "prisma generate --schema=src/prisma/schema.prisma",
    "prisma:push": "prisma db push --schema=src/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=src/prisma/schema.prisma",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "sam:build": "pnpm run build && cd src/lambda && sam build",
    "sam:deploy": "pnpm run build && cd src/lambda && sam deploy",
    "clean": "pnpm exec rimraf dist coverage .aws-sam node_modules/.cache",
    "clean:all": "pnpm run clean && pnpm exec rimraf node_modules",
    "postinstall": "pnpm run prisma:generate",
    "version:update": "tsx scripts/08-memoria/update-version.ts",
    "memory:update": "tsx scripts/08-memoria/update-memory.ts",
    "memory:sync": "pnpm run version:update && pnpm run memory:update"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.913.0",
    "@aws-sdk/client-dynamodb": "^3.621.0",
    "@aws-sdk/lib-dynamodb": "^3.621.0",
    "@fastify/aws-lambda": "^4.1.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^12.0.0",
    "@fastify/multipart": "^9.3.0",
    "@fastify/static": "^8.3.0",
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^4.1.0",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.8",
    "@nestjs/platform-fastify": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "^6.17.1",
    "@types/multer": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cloudinary": "^2.8.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "jwks-rsa": "^3.2.0",
    "multer": "^2.0.2",
    "nanoid": "^5.0.4",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pino": "^10.1.0",
    "pino-pretty": "^10.3.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/testing": "^11.1.6",
    "@playwright/test": "^1.56.1",
    "@types/aws-lambda": "^8.10.143",
    "@types/jest": "^29.5.12",
    "@types/minimatch": "^6.0.0",
    "@types/node": "^22.2.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prisma": "^6.17.1",
    "rimraf": "^5.0.5",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4"
  }
}
